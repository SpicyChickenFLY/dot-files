exec --no-startup-id export XIM=fcitx
exec --no-startup-id export XIM_PROGRAM=fcitx
exec --no-startup-id export INPUT_METHOD=fcitx
exec --no-startup-id export SDL_IM_MODULE=fcitx
exec --no-startup-id export GDK_BACKEND=x11

exec --no-startup-id xclip
exec --no-startup-id greenclip daemon 
